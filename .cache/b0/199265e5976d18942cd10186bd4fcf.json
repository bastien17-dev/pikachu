\n}\n\n@keyframes loose {\n  from {\n    transform: rotateX(-180deg);\n  }\n  50% {\n    transform: rotateX(-180deg) translateY(250px);\n  }\n  to {\n    transform: rotateX(-180deg) translateY(-350px);\n  }\n}\n\n@keyframes mountain-move {\n  100% {\n    left: -400px;\n  }\n}\n\n@keyframes cactus-move {\n  100% {\n    left: -10px;\n  }\n}\n","style/background.scss":"$game-width: 700px;\n\nbody {\n  background-color: black;\n  display: flex;\n}\n\n.game {\n  margin: auto;\n  position: relative;\n  width: $game-width;\n  height: 500px;\n  border: 1px solid black;\n  overflow: hidden;\n  background-color: royalblue;\n}\n\n.game__ground {\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n  height: 100px;\n  background-color: coral;\n}\n","style/mountains.scss":".game__mountain {\n  position: relative;\n  width: 100%;\n  height: 400px;\n}\n\n.mountain {\n  position: absolute;\n  left: 100%;\n  bottom: 0;\n  clip-path: polygon(49% 1%, 0% 100%, 100% 100%);\n  background: linear-gradient(to bottom, white, 5%, seagreen);\n}\n\n.mountain.move {\n  animation: mountain-move 80s linear;\n}\n\n.mountain1 {\n  width: 300px;\n  height: 250px;\n  left: 0px;\n}\n","style/cactus.scss":".game__cactus {\n  position: absolute;\n  width: 20px;\n  height: 50px;\n  bottom: 100px;\n  left: 100%;\n  background-image: url(\"../images/cactus.png\");\n  background-size: 100%;\n}\n\n.game__cactus.is-moving {\n  animation: cactus-move 10s linear;\n}\n","style/pikachu.scss":".game__pikachu {\n  position: absolute;\n  width: 50px;\n  height: 40px;\n  background-image: url(\"../images/pikachuStart.png\");\n  background-size: 100%;\n  background-repeat: no-repeat;\n  background-position: bottom;\n  bottom: 100px;\n  left: 50px;\n}\n\n.game__pikachu.is-running {\n  background-image: url(\"../images/pikachu.gif\");\n}\n\n.game__pikachu.is-jumping {\n  animation: jump 1.5s ease-out;\n  background-image: url(\"../images/pikachuJump.png\");\n}\n\n.game__pikachu.is-dead {\n  animation: loose 1.5s ease-out forwards;\n}\n","style/items.scss":".start {\n  position: absolute;\n  bottom: 10px;\n  left: 10px;\n  border: none;\n  border-radius: 5px;\n  background-color: white;\n  width: 150px;\n  height: 30px;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n  font-size: 1.2em;\n  letter-spacing: 2px;\n}\n\n.game__score {\n  position: absolute;\n  right: 10px;\n  bottom: 15px;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n}\n","style/overlay.scss":".overlay {\n  display: flex;\n  position: absolute;\n  top: 0;\n  transform: translateY(-100%);\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.2);\n  transition: transform 1s linear;\n}\n.overlay.is-visible {\n  transform: none;\n}\n.overlay__text {\n  margin: auto;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n  font-size: 60px;\n  color: crimson;\n}\n"},"lineCount":null}},"error":null,"hash":"4ac6c95f084d09bf32a582d778dc9819","cacheData":{"env":{}}}