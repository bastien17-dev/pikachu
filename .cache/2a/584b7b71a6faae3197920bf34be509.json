lue;\n}\n\n.game__ground {\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n  height: 100px;\n  background-color: coral;\n}\n\n.game__sun {\n  position: absolute;\n  top: 150px;\n  left: -20px;\n  border-radius: 50%;\n  background-color: gold;\n  width: 50px;\n  height: 50px;\n  animation: sun-move 480s linear forwards;\n}\n","style/mountains.scss":".game__mountain {\n  position: relative;\n  width: 100%;\n  height: 400px;\n}\n\n.mountain {\n  position: absolute;\n  left: 100%;\n  bottom: 0;\n  -webkit-clip-path: polygon(49% 1%, 0% 100%, 100% 100%);\n  background: linear-gradient(to bottom, white, 5%, seagreen);\n}\n\n.mountain.move {\n  animation: mountain-move 80s linear forwards;\n}\n\n.mountain1 {\n  width: 300px;\n  height: 250px;\n  left: 0px;\n}\n","style/cactus.scss":".game__cactus {\n  position: absolute;\n  width: 20px;\n  height: 50px;\n  bottom: 100px;\n  left: 100%;\n  background-image: url(\"../images/cactus.png\");\n  background-size: 100%;\n}\n\n.game__cactus.is-moving {\n  animation: cactus-move 10s linear;\n}\n","style/pikachu.scss":".game__pikachu {\n  position: absolute;\n  width: 50px;\n  height: 40px;\n  background-image: url(\"../images/pikachuStart.png\");\n  background-size: 100%;\n  background-repeat: no-repeat;\n  background-position: bottom;\n  bottom: 100px;\n  left: 50px;\n}\n\n.game__pikachu.is-running {\n  background-image: url(\"../images/pikachu.gif\");\n}\n\n.game__pikachu.is-flashing {\n  animation: is-flashing 0.5s alternate infinite;\n}\n\n.game__pikachu.is-jumping {\n  animation: jump 1s;\n  background-image: url(\"../images/pikachuJump.png\");\n}\n\n.game__pikachu.is-jumping.is-flashing {\n  animation: jump 1s, is-flashing 0.5s alternate infinite;\n  background-image: url(\"../images/pikachuJump.png\");\n}\n\n.game__pikachu.is-dead {\n  animation: loose 1.5s ease-out forwards;\n}\n","style/items.scss":".start {\n  position: absolute;\n  bottom: 10px;\n  left: 10px;\n  border: none;\n  border-radius: 5px;\n  background-color: white;\n  width: 150px;\n  height: 30px;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n  font-size: 1.2em;\n  letter-spacing: 2px;\n}\n\n.game__score {\n  position: absolute;\n  right: 10px;\n  bottom: 10px;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n}\n\n.game__life {\n  position: absolute;\n  bottom: 10px;\n  left: 45%;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n  font-size: 1.5em;\n}\n","style/overlay.scss":".overlay {\n  display: flex;\n  position: absolute;\n  top: 0;\n  transform: translateY(-100%);\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.2);\n  transition: transform 1s linear;\n}\n.overlay.is-visible {\n  transform: none;\n}\n.overlay__text {\n  margin: auto;\n  font-family: Impact, Haettenschweiler, \"Arial Narrow Bold\", sans-serif;\n  font-size: 60px;\n  color: crimson;\n}\n"},"lineCount":null}},"error":null,"hash":"d887499720882c82000f35bd373028c9","cacheData":{"env":{}}}